{
  "name": "bootstrap-parcel-bytesize-icons",
  "version": "0.1.0",
  "description": "Bootstrap 4.x Template with Parcel Bundler and Bytesize Icons",
  "private": true,
  "scripts": {
    "build": "del-cli dist && parcel build ./**/*.html -d dist --no-source-maps --detailed-report",
    "clear": "del-cli dist",
    "serve": "del-cli dist && parcel ./**/*.html -d dist"
  },
  "keywords": [
    "bootstrap",
    "parcel",
    "bytesize-icons"
  ],
  "author": "Marco Lehmann <m99coder@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.5.0",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "del-cli": "^3.0.1",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-imagemin": "^4.0.1",
    "parcel-plugin-purgecss": "^3.0.0",
    "sass": "^1.26.7"
  }
}
